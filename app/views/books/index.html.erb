<h1 class= "index-title">Books</h1>

<% @books.each do |book| %>
  <h4>Title</h4>
  <span><%= book.title %></span>

  <h4>Body</h4>
  <span><%= book.body %></span>
<% end %>

<h1>リンクを作る！！</h1>



<%= form_with model: Book.new do |f| %>
  <p>Title<p>
  <%= f.text_field :title %>

  <p>body<p>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>